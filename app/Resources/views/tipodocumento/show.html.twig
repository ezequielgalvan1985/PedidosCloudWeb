{% extends 'base.html.twig' %}
{% form_theme delete_form 'bootstrap_3_layout.html.twig' %}
{% block body %}
    <h1>Tipodocumento</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipodocumento.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ tipodocumento.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ tipodocumento.descripcion }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipodocumento_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tipodocumento_edit', { 'id': tipodocumento.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
