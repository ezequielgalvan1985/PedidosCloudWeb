{% extends 'base.html.twig' %}

{% block body %}
    <h1>Condicioniva</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ condicioniva.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ condicioniva.nombre }}</td>
            </tr>
            <tr>
                <th>Alicuota</th>
                <td>{{ condicioniva.alicuota }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('condicioniva_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('condicioniva_edit', { 'id': condicioniva.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
